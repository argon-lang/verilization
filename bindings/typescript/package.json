{
  "name": "@verilization/compiler",
  "version": "0.1.0",
  "description": "Compiler for verilization.",
  "type": "module",
  "main": "bin/index.js",
  "exports": {
    ".": "./bin/index.js"
  },
  "scripts": {
    "build-wasm": "cd ../../compiler && cargo +nightly build --lib --release --target wasm32-unknown-unknown",
    "build": "npm run build-wasm && mkdirp bin && cpy ../../compiler/target/wasm32-unknown-unknown/release/verilization_compiler.wasm bin/ && tsc --project tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "cpy-cli": "^3.1.1",
    "mkdirp": "^1.0.4",
    "typescript": "^4.2.4"
  }
}
